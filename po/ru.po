#: src/help_screen.c:79
msgid "q:\t\t Quit program"
msgstr "q:\t\t Выйти из программы"

#: src/help_screen.c:80
msgid "j or <Down>:\t Move one line down"
msgstr "j или <Down>:\t Спуститься на одну строку"

#: src/help_screen.c:81
msgid "k or <Up>:\t Move one line up"
msgstr "k или <Up>:\t Подняться на одну строку"

#: src/help_screen.c:82
msgid "u or <PgUp>:\t Move one screen up"
msgstr "u или <PgUp>:\t Подняться на один экран"

#: src/help_screen.c:83
msgid "d or <PgDn>:\t Move one screen down"
msgstr "d или <PgDn>:\t Спуститься на один экран"

#: src/help_screen.c:84
msgid "G:\t\t Move to play list end"
msgstr "G:\t\t Переместиться в конец плей-листа"

#: src/help_screen.c:85
msgid "<Number>G:\t Move to line <Number>"
msgstr "<Число>G:\t Переместиться на строку <Число>"

#: src/help_screen.c:86
msgid "J:\t\t Move play list one line down"
msgstr "J:\t\t Опустить плей-лист на строку"

#: src/help_screen.c:87
msgid "K:\t\t Move play list one line up"
msgstr "K:\t\t Поднять плей-лист на строку"

#: src/help_screen.c:88
msgid "<Number>M:\t Move play list to line <Number>"
msgstr "<Число>M:\t Переместить плей-лист в строку <Число>"

#: src/help_screen.c:89
msgid "<Ret>:\t\t Start playing"
msgstr "<Ret>:\t\t Начать воспроизведение"

#: src/help_screen.c:90
msgid "x:\t\t Play stopped song or start playing from begin"
msgstr "x:\t\t Воспроизводит остановленную песню или начать сначала"

#: src/help_screen.c:92
msgid "c:\t\t Pause/unpause"
msgstr "c:\t\t Пауза/продолжение"

#: src/help_screen.c:93
msgid "v:\t\t Stop playing"
msgstr "v:\t\t Остановить воспроизведение"

#: src/help_screen.c:94
msgid "b:\t\t Next song"
msgstr "b:\t\t Следующая песня"

#: src/help_screen.c:95
msgid "z:\t\t Previous song"
msgstr "z:\t\t Предыдущая песня"

#: src/help_screen.c:96
msgid "<:\t\t Scroll song 10 seconds up"
msgstr "h:\t\t Сместиться на 10 секунд назад"

#: src/help_screen.c:97
msgid ">:\t\t Scroll song 10 seconds down"
msgstr "l:\t\t Сместиться на 10 секунд вперед"

#: src/help_screen.c:98
msgid "<Number>t:\t Go to <Number>th second in song"
msgstr "<Число>g:\t Сместиться к секунде <Число>"

#: src/help_screen.c:99
msgid "+:\t\t Increase volume"
msgstr "+:\t\t Увеличить громкость"

#: src/help_screen.c:100
msgid "-:\t\t Decrease volume"
msgstr "-:\t\t Уменьшить громкость"

#: src/help_screen.c:101
msgid "]:\t\t Increase balance"
msgstr "]:\t\t Увеличить баланс"

#: src/help_screen.c:102
msgid "[:\t\t Decrease balance"
msgstr "]:\t\t Уменьшить баланс"

#: src/help_screen.c:103
msgid "i:\t\t Song info editor"
msgstr "i:\t\t Редактор информации о песне"

#: src/help_screen.c:104
msgid "a:\t\t Add songs"
msgstr "a:\t\t Добавить песни"

#: src/help_screen.c:105
msgid "A:\t\t Add object"
msgstr "A:\t\t Добавить объект"

#: src/help_screen.c:106
msgid "r:\t\t Remove songs"
msgstr "r:\t\t Удалить песни"

#: src/help_screen.c:107
msgid "s:\t\t Save play list"
msgstr "s:\t\t Сохранить плей-лист"

#: src/help_screen.c:108
msgid "S:\t\t Sort play list"
msgstr "S:\t\t Сортировать плей-лист"

#: src/help_screen.c:109
msgid "V:\t\t Start visual mode"
msgstr "V:\t\t Перейти в визуальный режим"

#: src/help_screen.c:110
msgid "C:\t\t Centrize view"
msgstr "C:\t\t Центрировать экран"

#: src/help_screen.c:111
msgid "/:\t\t Search"
msgstr "/:\t\t Поиск"

#: src/help_screen.c:112
msgid "\\:\t\t Advanced search"
msgstr "\\:\t\t Продвинутый поиск"

#: src/help_screen.c:113
msgid "n:\t\t Go to next search match"
msgstr "n:\t\t Перейти к следующему результату поиска"

#: src/help_screen.c:114
msgid "N:\t\t Go to previous search match"
msgstr "N:\t\t Перейти в предыдущему результату поиска"

#: src/help_screen.c:115
msgid "e:\t\t Launch equalizer window"
msgstr "e:\t\t Вызвать окно эквалайзера"

#: src/help_screen.c:116
msgid "R:\t\t Set/unset shuffle play mode"
msgstr "R:\t\t Устаносить/сбросить случайный режим воспроизведения"

#: src/help_screen.c:117
msgid "L:\t\t Set/unset loop play mode"
msgstr "L:\t\t Установить/сбросить кольцевой режим воспроизведения"

#: src/help_screen.c:118
msgid "o:\t\t Variables mini-manager"
msgstr "o:\t\t Мини-менеджер переменных"

#: src/help_screen.c:119
msgid "O:\t\t Advanced variables manager"
msgstr "O:\t\t Продвинутый менеджер переменных"

#: src/help_screen.c:120
msgid "U:\t\t Undo"
msgstr "U:\t\t Отменить"

#: src/help_screen.c:121
msgid "R:\t\t Redo"
msgstr "R:\t\t Восстановить"

#: src/help_screen.c:122
msgid "I:\t\t Reload songs information"
msgstr "I:\t\t Перезагрузить информацию о песнях"

#: src/help_screen.c:123
msgid "P:\t\t Reload plugins"
msgstr "P:\t\t Перезагрузить плагины"

#: src/help_screen.c:124
msgid "ps:\t\t Set play bounds"
msgstr "ps:\t\t Установить границы воспроизведения"

#: src/help_screen.c:125
msgid "p<Ret>:\t\t Set play bounds and play"
msgstr "p<Ввод>:\t Установить границы и начать воспроизведение"

#: src/help_screen.c:126
msgid "pc:\t\t Clear play bounds"
msgstr "pc:\t\t Сбросить границы воспроизведения"

#: src/help_screen.c:127
msgid "!:\t\t Execute command"
msgstr "!:\t\t Выполнить команду"

#: src/help_screen.c:128
msgid "m<Letter>:\t Set mark <Letter>"
msgstr "m<Буква>:\t Установить метку <Буква>"

#: src/help_screen.c:129
msgid "`<Letter>:\t Go to mark <Letter>"
msgstr "`<Буква>:\t Перейти к метке <Буква>"

#: src/help_screen.c:130
msgid "``:\t\t Go to previous position"
msgstr "``:\t\t Перейти в предыдущей позиции"

#: src/help_screen.c:131
msgid "<Backspace>:\t Go to previous time"
msgstr "<Backspace>:\t Перейти к предыдущему времени"

#: src/help_screen.c:132
msgid "^l:\t\t Redisplay screen"
msgstr "^l:\t\t Обновить экран"

