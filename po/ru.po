#: ../src/help_screen.c:78
msgid "q:\t\t Quit program"
msgstr "q:\t\t Выйти из программы"

#: ../src/help_screen.c:79
msgid "j or <Down>:\t Move one line down"
msgstr "j или <Down>:\t Спуститься на одну строку"

#: ../src/help_screen.c:80
msgid "k or <Up>:\t Move one line up"
msgstr "k или <Up>:\t Подняться на одну строку"

#: ../src/help_screen.c:81
msgid "u or <PgUp>:\t Move one screen up"
msgstr "u или <PgUp>:\t Подняться на один экран"

#: ../src/help_screen.c:82
msgid "d or <PgDn>:\t Move one screen down"
msgstr "d или <PgDn>:\t Спуститься на один экран"

#: ../src/help_screen.c:83
msgid "G:\t\t Move to play list end"
msgstr "G:\t\t Переместиться в конец плей-листа"

#: ../src/help_screen.c:84
msgid "<Number>G:\t Move to line <Number>"
msgstr "<Число>G:\t Переместиться на строку <Число>"

#: ../src/help_screen.c:85
msgid "<Ret>:\t\t Start playing"
msgstr "<Ret>:\t\t Начать воспроизведение"

#: ../src/help_screen.c:86
msgid "x:\t\t Play stopped song or start playing from begin"
msgstr "x:\t\t Воспроизводит остановленную песню или начать сначала"

#: ../src/help_screen.c:88
msgid "c:\t\t Pause/unpause"
msgstr "c:\t\t Пауза/продолжение"

#: ../src/help_screen.c:89
msgid "v:\t\t Stop playing"
msgstr "v:\t\t Остановить воспроизведение"

#: ../src/help_screen.c:90
msgid "b:\t\t Next song"
msgstr "b:\t\t Следующая песня"

#: ../src/help_screen.c:91
msgid "z:\t\t Previous song"
msgstr "z:\t\t Предыдущая песня"

#: ../src/help_screen.c:92
msgid "h:\t\t Scroll song 10 seconds up"
msgstr "h:\t\t Сместиться на 10 секунд назад"

#: ../src/help_screen.c:93
msgid "l:\t\t Scroll song 10 seconds down"
msgstr "l:\t\t Сместиться на 10 секунд вперед"

#: ../src/help_screen.c:94
msgid "<Number>g:\t Go to <Number>th second in song"
msgstr "<Число>g:\t Сместиться к секунде <Число>"

#: ../src/help_screen.c:95
msgid "+:\t\t Increase volume"
msgstr "+:\t\t Увеличить громкость"

#: ../src/help_screen.c:96
msgid "-:\t\t Decrease volume"
msgstr "-:\t\t Уменьшить громкость"

#: ../src/help_screen.c:97
msgid "i:\t\t Song info editor"
msgstr "i:\t\t Редактор информации о песне"

#: ../src/help_screen.c:98
msgid "a:\t\t Add songs"
msgstr "a:\t\t Добавить песни"

#: ../src/help_screen.c:99
msgid "r:\t\t Remove songs"
msgstr "r:\t\t Удалить песни"

#: ../src/help_screen.c:100
msgid "s:\t\t Save play list"
msgstr "s:\t\t Сохранить плей-лист"

#: ../src/help_screen.c:101
msgid "S:\t\t Sort play list"
msgstr "S:\t\t Сортировать плей-лист"

#: ../src/help_screen.c:102
msgid "V:\t\t Start visual mode"
msgstr "V:\t\t Перейти в визуальный режим"

#: ../src/help_screen.c:103
msgid "C:\t\t Centrize view"
msgstr "C:\t\t Центрировать экран"

#: ../src/help_screen.c:104
msgid "/:\t\t Search"
msgstr "/:\t\t Поиск"

#: ../src/help_screen.c:105
msgid "n:\t\t Go to next search match"
msgstr "n:\t\t Перейти к следующему результату поиска"

#: ../src/help_screen.c:106
msgid "N:\t\t Go to previous search match"
msgstr "N:\t\t Перейти в предыдущему результату поиска"

#: ../src/help_screen.c:107
msgid "?:\t\t This help screen"
msgstr "?:\t\t Этот экран помощи"

